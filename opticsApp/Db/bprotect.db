grecord(swait,"$(P)bthetaAbleWait")
{
	field(DESC,"disable btheta")
	field(SCAN,"I/O Intr")
	field(FLNK,"$(P)bthetaAbleCalc.PROC PP MS")
	field(INAN,"$(P)$(M_BTRANS).DMOV")
	field(INBP,"No")
	field(PREC,"2")
}
grecord(swait,"$(P)btransAbleWait")
{
	field(DESC,"disable btrans")
	field(SCAN,"I/O Intr")
	field(FLNK,"$(P)btransAbleCalc.PROC PP MS")
	field(INAN,"$(P)$(M_BTHETA).DMOV")
	field(INBP,"No")
	field(PREC,"2")
}
grecord(seq,"$(P)bthetaAble")
{
	field(LNK1,"$(P)$(M_BTHETA)_able.DISA NPP NMS")
	field(DOL2,"$(P)bthetaAbleCalc.VAL NPP NMS")
	field(LNK2,"$(P)$(M_BTHETA)_able.VAL PP MS")
	field(DOL3,"$(P)bthetaAbleCalc.VAL NPP NMS")
	field(LNK3,"$(P)$(M_BTHETA)_able.DISA PP MS")
}
grecord(seq,"$(P)btransAble")
{
	field(LNK1,"$(P)$(M_BTRANS)_able.DISA NPP NMS")
	field(DOL2,"$(P)btransAbleCalc.VAL PP MS")
	field(LNK2,"$(P)$(M_BTRANS)_able.VAL PP MS")
	field(DOL3,"$(P)btransAbleCalc.VAL PP MS")
	field(LNK3,"$(P)$(M_BTRANS)_able.DISA PP MS")
}
grecord(calc,"$(P)bthetaAbleCalc")
{
	field(PINI,"YES")
	field(FLNK,"$(P)bthetaAble.PROC PP MS")
	field(CALC,"(a>15)&&b")
	field(INPA,"$(P)$(M_BTRANS).RBV NPP NMS")
	field(INPB,"$(P)bProtect.VAL NPP NMS")
}
grecord(calc,"$(P)btransAbleCalc")
{
	field(PINI,"YES")
	field(FLNK,"$(P)btransAble.PROC PP MS")
	field(CALC,"(ABS(A)>1)&&b")
	field(INPA,"$(P)$(M_BTHETA).RBV NPP NMS")
	field(INPB,"$(P)bProtect.VAL NPP NMS")
}
grecord(bo,"$(P)bProtect")
{
	field(FLNK,"$(P)bProtectFO.PROC PP MS")
	field(DOL,"1")
	field(ZNAM,"Unprotect")
	field(ONAM,"Protect")
}
grecord(fanout,"$(P)bProtectFO")
{
	field(LNK1,"$(P)bthetaAbleCalc.PROC PP MS")
	field(LNK2,"$(P)btransAbleCalc.PROC PP MS")
}
